{
    "name": "jfalque/http-mock",
    "description": "Allows to create HTTP fixtures to mock tests",
    "keywords": ["HTTP", "fixtures", "mock", "test"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "guzzlehttp/psr7": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Jfalque\\HttpMock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jfalque\\HttpMock\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
