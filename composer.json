{
    "name": "jfalque/http-mock",
    "description": "Mocks HTTP calls for tests",
    "keywords": ["HTTP", "mock", "test"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7",
        "guzzlehttp/psr7": "~1.0",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.0.*",
        "mikey179/vfsStream": "1.6.*",
        "phpunit/phpunit": "5.7.*"
    },
    "autoload": {
        "psr-4": {
            "Jfalque\\HttpMock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jfalque\\HttpMock\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
